def VvodStart(event):
        try:
                x=int(VvodBox.get())
                root = Tk()
                root.title(u'Справочник')
                global base
                base=[]                
                for i in range(x):
                        name=names[random.randint(0, len(names)-1)]
                        surname=surnames[random.randint(0, len(surnames)-1)]                    
                        if name[-1]=='а' or name[-1]=='я':
                                surname+='а'
                        base+=[[surname,name,street[random.randint(0, len(names)-1)],random.randint(1, 300),random.randint(1, 300),random.randint(80000000000, 89999999999)]]
                       
                def Base(event):
                        rootBase = Tk()
                        rootBase.title(u'Все записи')
                                                
                        SignExit = Frame(rootBase, bg = 'gray')
                        Signs = Frame(rootBase, bg = 'gray')

                        SignExit.pack(side = 'bottom', expand = 1)
                        Signs.pack(side = 'top', fill = 'both', expand = 1)
                        
                        Sign = Text(Signs,font="12",wrap=WORD,bg = 'white')
                        Sign.pack(side = 'left', fill = 'both', expand = 1)

                        scrollbar = Scrollbar(Signs)
                        scrollbar.pack(side = 'right', fill = 'y', expand = 1)
                        
                        Sign.config(yscrollcommand = scrollbar.set)
                        scrollbar.config(command = Sign.yview)
                        for i in range(len(base)):                            
                                Sign.insert(END,'\nИмя: ')
                                Sign.insert(END,base[i][0])
                                Sign.insert(END,' ')
                                Sign.insert(END,base[i][1])
                                Sign.insert(END,'\nАдрес: ')
                                Sign.insert(END,base[i][2])
                                Sign.insert(END,', дом ')
                                Sign.insert(END,base[i][3])
                                Sign.insert(END,', квартира ')
                                Sign.insert(END,base[i][4])
                                Sign.insert(END,'\nНомер телефона: ')
                                Sign.insert(END,base[i][5])
                                Sign.insert(END,'\n')
                                
                                i+=1
                                
                        def ExitS(event):
                                import time
                                time.sleep(0.5)
                                rootBase.destroy()
                        
                        ExitSign = Button(SignExit,text='Закрыть',width=30,height=2,bg="white",fg="black")
                        ExitSign.pack(side = 'bottom')
                        ExitSign.bind('<Button-1>', ExitS)
                
                def AddNew(event):                        
                        
