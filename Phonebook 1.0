                        def AcceptA(event):
                                try:
                                        global base
                                        len(base)==len(base)+1
                                        SurnameNew = EnterSurname.get().capitalize()
                                        NameNew = EnterName.get().capitalize()
                                        StreetNew = EnterStreet.get().capitalize()
