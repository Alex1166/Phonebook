                        EnterName.pack(side = 'left', fill = 'both', expand = 1)

                        FrStreet = LabelFrame(rootAdd, bg = 'Gainsboro', text="Название улицы: ")
                        FrStreet.pack(fill="both", expand="yes")
                        EnterStreet = Entry(FrStreet,width=25,bd=2)
                        EnterStreet.pack(side = 'left', fill = 'both', expand = 1)
                        

                        FrHouse = LabelFrame(rootAdd, bg = 'Gainsboro', text="Номер дома: ")
                        FrHouse.pack(fill="both", expand="yes")
                        EnterHouse = Entry(FrHouse,width=25,bd=2)
                        EnterHouse.pack(side = 'left', fill = 'both', expand = 1)

                        FrFlat = LabelFrame(rootAdd, bg = 'Gainsboro', text="Номер квартиры: ")
