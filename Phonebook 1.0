                def Search(event):

                        rootSearchSurname = Tk()
                        rootSearchSurname.title(u'Поиск записи по фамилии')

                        FrSurnameSearch = LabelFrame(rootSearchSurname, bg = 'Gainsboro', text="Введите фамилию: ")
                        FrSurnameSearch.pack(fill="both", expand="yes")
                        SurnameSearchSTR = StringVar()
                        EnterSurnameSearch = Entry(FrSurnameSearch, width=25, bd=2, textvariable=SurnameSearchSTR)
                        EnterSurnameSearch.pack(side = 'left', fill = 'both', expand = 1)

                        SearchExit = Frame(rootSearchSurname, bg = 'Gainsboro')
                        SearchExit.pack(side = 'top', expand = 1)

                        def AcceptSearch(event):
                                SurnameSearch = SurnameSearchSTR.get().capitalize()
                                rootSearchR = Tk()
                                rootSearchR.title(u'Результат поиска')

                                SearchResultExit = Frame(rootSearchR, bg = 'gray')
                                SearchResults = Frame(rootSearchR, bg = 'gray')

                                SearchResultExit.pack(side = 'bottom', expand = 1)
                                SearchResults.pack(side = 'top', fill = 'both', expand = 1)

                                scrollbar = Scrollbar(SearchResults)
                                scrollbar.pack(side = 'right', fill = 'y', expand = 1)

                                SearchResult = Text(SearchResults,font="12",wrap=WORD,bg = 'white')
                                SearchResult.pack(side = 'left', fill = 'both', expand = 1)
                                
                                SearchResult.config(yscrollcommand = scrollbar.set)
                                scrollbar.config(command = SearchResult.yview)
                                
                                j=0
                                for j in range(len(base)):
                                        if SurnameSearch in base[j][0]:
                                                
                                                SearchResult.insert(END,'\nИмя: ')
                                                SearchResult.insert(END,base[j][0])
                                                SearchResult.insert(END,' ')
                                                SearchResult.insert(END,base[j][1])
                                                SearchResult.insert(END,'\nАдрес: ')
                                                SearchResult.insert(END,base[j][2])
                                                SearchResult.insert(END,', дом ')
                                                SearchResult.insert(END,base[j][3])
                                                SearchResult.insert(END,', квартира ')
                                                SearchResult.insert(END,base[j][4])
                                                SearchResult.insert(END,'\nНомер телефона: ')
                                                SearchResult.insert(END,base[j][5])
                                                SearchResult.insert(END,'\n')                                                
                                                
                                def ExitS(event):
                                        import time
                                        time.sleep(0.5)
                                        rootSearchR.destroy()
                                
                                ExitSearchResult = Button(SearchResultExit,text='Закрыть',width=30,height=2,bg="white",fg="black")
                                ExitSearchResult.pack(side = 'bottom')
                                ExitSearchResult.bind('<Button-1>', ExitS)
                                
                                import time
                                time.sleep(0.5)
                                rootSearchSurname.destroy()
                            

                        SearchBTN = Button(SearchExit,text='Найти',width=30,height=2,bg="white",fg="black")
