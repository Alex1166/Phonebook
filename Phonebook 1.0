                                time.sleep(0.5)
                                rootSearchSurname.destroy()
                        
                        ExitSearch = Button(SearchExit,text='Закрыть',width=30,height=2,bg="white",fg="black")
                        ExitSearch.pack(side = 'left')
                        ExitSearch.bind('<Button-1>', ExitS)
                        
                rootStart.destroy()
                        
